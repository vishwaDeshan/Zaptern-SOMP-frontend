{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@zaptern-somp-frontend/applicants-list": [
        "libs/applicants-list/src/index.ts"
      ],
      "@zaptern-somp-frontend/auto-savable-form": [
        "libs/shared/auto-savable-form/src/index.ts"
      ],
      "@zaptern-somp-frontend/components": [
        "libs/shared/components/src/index.ts"
      ],
      "@zaptern-somp-frontend/data-access": [
        "libs/somp-portal/data-access/src/index.ts"
      ],
      "@zaptern-somp-frontend/directives": [
        "libs/shared/directives/src/index.ts"
      ],
      "@zaptern-somp-frontend/feature": [
        "libs/somp-portal/feature/src/index.ts"
      ],
      "@zaptern-somp-frontend/form-container": [
        "libs/shared/components/form-container/src/index.ts"
      ],
      "@zaptern-somp-frontend/helpers": ["libs/shared/helpers/src/index.ts"],
      "@zaptern-somp-frontend/model": ["libs/somp-portal/model/src/index.ts"],
      "@zaptern-somp-frontend/common/models": ["libs/shared/models/src/index.ts"],
      "@zaptern-somp-frontend/services": ["libs/shared/services/src/index.ts"],
      "@zaptern-somp-frontend/shared-data-access": [
        "libs/shared/shared-data-access/src/index.ts"
      ],
      "@zaptern-somp-frontend/student-list": ["libs/student-list/src/index.ts"],
      "@zaptern-somp-frontend/ui": ["libs/shared/ui/src/index.ts"],
      "@zaptern-somp-frontend/utill": ["libs/somp-portal/utill/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
